{
  "name": "css-to-typed",
  "displayName": "CSS to Typed",
  "description": "css classNames export .d.ts",
  "version": "0.1.17",
  "publisher": "mineclover",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mineclover/css-to-typed.git"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onCommand:extension.copyHoveredType",
    "onCommand:variable-name-suggester.suggestNames",
    "onView:variableNameSuggesterSidebar"
  ],
  "main": "./out/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "variable-name-suggester-sidebar",
          "title": "Variable Name Suggester",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "variable-name-suggester-sidebar": [
        {
          "id": "variableNameSuggesterSidebar",
          "name": "Variable Names"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.createType",
        "title": "CSS to Typed: create a css class type"
      },
      {
        "command": "extension.clipboardType",
        "title": "CSS to Typed: copy a css class type"
      },
      {
        "command": "extension.copyInferredType",
        "title": "CSS to Typed: Copy Inferred Type"
      },
      {
        "command": "variable-name-suggester.suggestNames",
        "title": "Suggest Variable Names"
      }
    ],
    "configuration": {
      "title": "CSS to Typed",
      "properties": {
        "cssToTyped.showTypeOnHover": {
          "type": "boolean",
          "default": false,
          "description": "Show inferred type on hover"
        },
        "variableNameSuggester.apiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for OpenAI GPT"
        },
        "variableNameSuggester.suggestionCount": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 10,
          "description": "Number of variable name suggestions to generate (1-10)"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "variable-name-suggester.suggestNames",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.26.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "typescript": "^5.5.3"
  }
}
